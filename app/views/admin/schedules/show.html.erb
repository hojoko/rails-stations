<div>上映情報</div>

<table>
  <thead>
    <tr>
      <td><h2>ID</h2></td>
      <td><h2>映画名</h2></td>
      <td><h2>開始時間</h2></td>
      <td><h2>終了時間</h2></td>
      <td><h2>作成日時</h2></td>
      <td><h2>更新日時</h2></td>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @schedule.id %></td>
      <td><%= @movie.name %></td>
      <td><%= l @schedule.start_time %></td>
      <td><%= l @schedule.end_time %></td>
      <td><%= l @schedule.created_at %></td>
      <td><%= l @schedule.updated_at %></td>
    </tr>
  </tbody>
</table>
<div><%= link_to "編集", edit_admin_schedule_path(@schedule) %></div>
<div><%= link_to "削除", admin_schedule_path(@schedule), method: :delete %></div>